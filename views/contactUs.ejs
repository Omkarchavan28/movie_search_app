<% include partials/header%>
<link rel="stylesheet" href="/css/contactUs.css">
<div class="container form">  
	<form id="contact" action="/contactUs" method="post">
	  <h3>Contact Us</h3>
	  <fieldset>
		<input name="username" placeholder="Your name" type="text" tabindex="1" required autofocus>
	  </fieldset>
	  <fieldset>
		<input name="email" placeholder="Your Email Address" type="email" tabindex="2" required>
	  </fieldset>
	  <fieldset>
		<input name="phone" placeholder="Your Phone Number (optional)" type="tel" tabindex="3" required>
	  </fieldset>
	
	  <fieldset>
		<textarea name="message" placeholder="Type your message here...." tabindex="5" required></textarea>
	  </fieldset>
	  <fieldset>
		<button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
	  </fieldset>
	</form>
  </div>